# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Model Configuration
TARGET_MODEL=gpt-3.5-turbo
JUDGE_MODEL=gpt-4o-mini

# Ollama Cloud Configuration (for evaluation)
USE_OLLAMA_FOR_EVALUATION=true
OLLAMA_API_KEY=your_ollama_api_key_here
OLLAMA_HOST=https://ollama.com
OLLAMA_JUDGE_MODEL=glm-4.6:cloud

# Authentication Configuration
VALID_EMAIL=your_admin_email@example.com
VALID_PASSWORD=your_secure_password_here
SESSION_EXPIRY_HOURS=24

# CORS Configuration (comma-separated for multiple origins)
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# Frontend API URL (for Next.js)
NEXT_PUBLIC_API_URL=http://localhost:8000

